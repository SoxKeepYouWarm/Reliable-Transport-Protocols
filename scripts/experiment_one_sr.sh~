#!/usr/bin/bash

rm -R ./experiment_one_sr
mkdir ./experiment_one_sr

CONSTANTS="-m 1000 -t 50 -c 0.2"
ERROR_LOG="./experiment_one_sr/error_log.txt"


for window in 10 50
do

for loss in 0.1 0.2 0.4 0.6 0.8
do

echo "starting experiment one SR: loss ${loss} window: ${window}"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l ${loss} -w ${window} -o ./experiment_one_sr/sr_l${loss}_w${window}.csv 2>> ${ERROR_LOG} &

done

done


echo "starting experiment one SR: loss 0.1 window: 10"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.1 -w 10 -o ./experiment_one_sr/sr_l0.1_w10.csv 2>> ${ERROR_LOG} &
 
echo "starting experiment one SR: loss 0.2 window: 10"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.2 -w 10 -o ./experiment_one_sr/sr_l0.2_w10.csv 2>> ${ERROR_LOG} &

echo "starting experiment one SR: loss 0.4 window: 10"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.4 -w 10 -o ./experiment_one_sr/sr_l0.4_w10.csv 2>> ${ERROR_LOG} &

echo "starting experiment one SR: loss 0.6 window: 10"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.6 -w 10 -o ./experiment_one_sr/sr_l0.6_w10.csv 2>> ${ERROR_LOG} &

echo "starting experiment one SR: loss 0.8 window: 10"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.8 -w 10 -o ./experiment_one_sr/sr_l0.8_w10.csv 2>> ${ERROR_LOG} &

echo "starting experiment one SR: loss 0.1 window: 50"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.1 -w 50 -o ./experiment_one_sr/sr_l0.1_w50.csv 2>> ${ERROR_LOG} &
 
echo "starting experiment one SR: loss 0.2 window: 50"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.2 -w 50 -o ./experiment_one_sr/sr_l0.2_w50.csv 2>> ${ERROR_LOG} &

echo "starting experiment one SR: loss 0.4 window: 50"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.4 -w 50 -o ./experiment_one_sr/sr_l0.4_w50.csv 2>> ${ERROR_LOG} &

echo "starting experiment one SR: loss 0.6 window: 50"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.6 -w 50 -o ./experiment_one_sr/sr_l0.6_w50.csv 2>> ${ERROR_LOG} &

echo "starting experiment one SR: loss 0.8 window: 50"
./run_experiments -p ../davidtow/sr ${CONSTANTS} -l 0.8 -w 50 -o ./experiment_one_sr/sr_l0.8_w50.csv 2>> ${ERROR_LOG} &

wait

echo "finished SR"

exit 0
