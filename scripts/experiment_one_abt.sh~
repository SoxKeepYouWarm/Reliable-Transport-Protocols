#!/usr/bin/bash

CONSTANTS = "-m 1000 -t 50 -c 0.2"

echo "starting experiment one ABT: loss 0.1"
./run_experiments -p ../davidtow/abt $(CONSTANTS) -l 0.1 &
 
echo "starting experiment one ABT: loss 0.2"
./run_experiments -p ../davidtow/abt $(CONSTANTS) -l 0.2 &

echo "starting experiment one ABT: loss 0.4"
./run_experiments -p ../davidtow/abt $(CONSTANTS) -l 0.4 &

echo "starting experiment one ABT: loss 0.6"
./run_experiments -p ../davidtow/abt $(CONSTANTS) -l 0.6 &

echo "starting experiment one ABT: loss 0.8"
./run_experiments -p ../davidtow/abt $(CONSTANTS) -l 0.8 &

wait

exit 0
